{
  "name": "dom99",
  "version": "20.0.3",
  "license": "BSL-1.0",
  "module": "built/dom99ES.js",
  "main": "built/dom99Require.js",
  "homepage": "https://dom99.now.sh/",
  "description": "Extend html with directives to manipulate the dom with JavaScript",
  "type": "module",
  "scripts": {
    "lint-fix": "eslint --fix source",
    "lint": "eslint source",
    "bundle": "rollup --config tools/rollup.config.js",
    "transpile": "babel built/dom99Script.js > built/dom99Script.es5.js",
    "minify": "terser built/dom99Script.es5.js --ie8 --compress drop_console,properties,dead_code,join_vars --mangle -o built/dom99Script.es5.min.js && terser built/dom99ES.js --type module --ie8 --toplevel --compress drop_console,properties,dead_code,join_vars --mangle -o built/dom99ES.min.js",
    "build": "npm run bundle && npm run transpile && npm run minify",
    "test": "tests/specification/SpecRunner.html",
    "transpile-polyfills": "babel polyfills --out-dir polyfills/built --ignore polyfills/built/",
    "bundle-examples": "node --experimental-modules tools/buildExamples.js",
    "transpile-examples": "babel examples/js/built --out-dir examples/js/builtes5",
    "prepare-examples": "node --experimental-modules ./tools/exampleDependencies.js",
    "build-examples": "npm run bundle-examples && npm run transpile-examples && npm run prepare-examples",
    "yes-no-example": "rollup components/yesNoDialog/examples/js/ScrollTest.js --o components/yesNoDialog/examples/js/built/ScrollTest-bundled.js --f iife && babel components/yesNoDialog/examples/js/built/ScrollTest-bundled.js > components/yesNoDialog/examples/js/built/ScrollTest.js",
    "build-doc-examples": "babel documentation/js/documentation.js > documentation/js/documentation.es5.min.js",
    "generate-html": "node --experimental-modules tools/buildHTML.js",
    "prepare-site": "node --experimental-modules tools/prepareSite.js",
    "jsdoc": "jsdoc source/dom99.js -t documentation/jsdoctemplate --destination ./documentation",
    "docs": "npm run build-doc-examples && npm run generate-html && npm run prepare-site && npm run jsdoc",
    "deploy": "cd documentation && now --public",
    "now": "cd documentation && now --public --target production",
    "alias-now": "now alias https://documentation-idlakfyzsr.now.sh dom99",
    "build-all": "npm run build && npm run lint && npm run transpile-polyfills && npm run build-examples && npm run yes-no-example && npm run docs",
    "most": "npm run lint && npm run build && npm run docs",
    "prepare": "npm run build",
    "serve-doc": "serve ./documentation -p 8080",
    "serve": "serve ./ -p 8080",
    "publish-as-latest": "npm publish",
    "publish-as-experimental": "npm publish --tag experimental"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "bootstrap": "^4.3.1",
    "browserify": "^16.3.0",
    "eslint": "^6.0.1",
    "filesac": "^11.0.0",
    "handlebars": "^4.1.2",
    "highlight.js": "^9.15.8",
    "html-minifier": "^4.0.0",
    "jasmine-core": "^3.4.0",
    "jsdoc": "^3.6.3",
    "rollup": "^1.17.0",
    "serve": "^11.1.0",
    "template-mb": "^2.0.6",
    "terser": "^4.1.2",
    "utilsac": "^10.4.1"
  },
  "files": [
    "built",
    "components",
    "source",
    "plugins",
    "polyfills",
    "LICENSE.txt",
    "readme.md"
  ],
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {}
    },
    "env": {
      "browser": true
    },
    "rules": {
      "no-console": "off",
      "constructor-super": 2,
      "for-direction": 2,
      "getter-return": 2,
      "no-case-declarations": 2,
      "no-class-assign": 2,
      "no-compare-neg-zero": 2,
      "no-cond-assign": 2,
      "no-const-assign": 2,
      "no-constant-condition": 2,
      "no-control-regex": 2,
      "no-debugger": 2,
      "no-delete-var": 2,
      "no-dupe-args": 2,
      "no-dupe-class-members": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty-character-class": 2,
      "no-empty-pattern": 2,
      "no-empty": 2,
      "no-ex-assign": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-semi": 2,
      "no-fallthrough": 2,
      "no-func-assign": 2,
      "no-global-assign": 2,
      "no-inner-declarations": 2,
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": 2,
      "no-mixed-spaces-and-tabs": 2,
      "no-new-symbol": 2,
      "no-obj-calls": 2,
      "no-octal": 2,
      "no-redeclare": 2,
      "no-regex-spaces": 2,
      "no-self-assign": 2,
      "no-sparse-arrays": 2,
      "no-this-before-super": 2,
      "no-undef": 2,
      "no-unexpected-multiline": 2,
      "no-unreachable": 2,
      "no-unsafe-finally": 2,
      "no-unsafe-negation": 2,
      "no-unused-labels": 2,
      "no-unused-vars": 2,
      "no-useless-escape": 2,
      "require-yield": 2,
      "use-isnan": 2,
      "valid-typeof": 2
    }
  },
  "babel": {
    "plugins": [],
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "targets": {
            "browsers": [
              "> 0.1%"
            ]
          }
        }
      ]
    ]
  },
  "keywords": [
    "dom99",
    "html",
    "javascript",
    "dom",
    "declarative",
    "custom",
    "elements",
    "web",
    "component",
    "framework",
    "library",
    "reactive",
    "data-attributes",
    "light",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/DOM99.git"
  }
}
