{
  "name": "dom99",
  "version": "1.23.9",
  "license": "MIT",
  "description": "HTML/DOM manipulation made easier",
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/DOM99.git"
  },
  "dependencies": {},
  "main": "js/dom99.js",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "uglify-js": "2.6.2"
  },
  "scripts": {
    "build": "babel js/dom99.js | uglifyjs - --screw-ie8 --compress drop_console,properties,dead_code,join_vars --mangle -o js/dom99.es5.min.js",
    "buildexamples": "babel examples -d exampleses5",
    "builddoc": "babel documentation/js/documentation.js | uglifyjs - --screw-ie8 --compress,properties,dead_code,join_vars --mangle -o documentation/js/documentation.es5.min.js"
  },
  "keywords": [
    "HTML",
    "DOM",
    "declarative",
    "ES2015",
    "HTML5",
    "custom",
    "elements",
    "web",
    "component",
    "fast",
    "light"
  ],
  "homepage": "https://cdn.rawgit.com/GrosSacASac/DOM99/1.23.2/documentation/documentation.html"
}
