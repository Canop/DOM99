{
  "name": "dom99",
  "version": "7.2.1",
  "license": "BSL-1.0",
  "licenseLink": "http://www.boost.org/LICENSE_1_0.txt",
  "description": "HTML/DOM manipulation made easier",
  "main": "built/dom99Require.js",
  "module": "built/dom99Module.js",
  "scriptFileBrowser": "js/dom99.js",
  "scriptFileBrowserES5": "built/dom99.es5.min.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/DOM99.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "uglify-js": "2.6.2",
    "utilsac": "^2.0.0"
  },
  "scripts": {
    "buildes5": "babel js/dom99.js > built/dom99.es5.js",
    "buildes5min": "babel minified=true js/dom99.js > built/dom99.es5.min.js",
    "buildes5minOLD": "uglifyjs built/dom99.es5.js --screw-ie8 --compress drop_console,properties,dead_code,join_vars --mangle -o built/dom99.es5.min.jss",
    "buildModuleAndRequire": "node tools/buildModuleAndRequire.js",
    "build": "npm run buildes5 && npm run buildminify && npm run buildModuleAndRequire",
    "buildexamples": "babel examples -d exampleses5",
    "builddoc": "babel documentation/js/documentation.js | uglifyjs - --screw-ie8 --compress,properties,dead_code,join_vars --mangle -o documentation/js/documentation.es5.min.js",
    "buildyesnodialogexample": "babel components/yesNoDialog/yesNoDialog.js > built/components/yesNoDialog/yesNoDialog.es5.js && babel components/yesNoDialog/examples/ScrollTest.js > built/components/yesNoDialog/examples/ScrollTest.es5.js"
  },
  "keywords": [
    "HTML",
    "DOM",
    "declarative",
    "custom",
    "elements",
    "web",
    "component",
    "fast",
    "light",
    "browser"
  ]
}
