{
  "name": "dom99",
  "version": "8.2.16",
  "license": "BSL-1.0",
  "main": "built/dom99Require.js",
  "module": "built/dom99Module.js",
  "scriptFileBrowser": "js/dom99.js",
  "scriptFileBrowserES5": "built/dom99.es5.min.js",
  "url": "https://cdn.rawgit.com/GrosSacASac/DOM99/master/documentation/documentation.html",
  "licenseLink": "http://www.boost.org/LICENSE_1_0.txt",
  "description": "HTML/DOM manipulation made easier",
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/DOM99.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^3.3.7",
    "html-minifier": "^3.5.10",
    "rollup": "^0.50.0",
    "uglify-js": "^3.1.5",
    "utilsac": "^2.0.0"
  },
  "scripts": {
    "transpile": "babel js/dom99.js > built/dom99.es5.js",
    "transpileminFAIL": "babel minified=true js/dom99.js > built/dom99.es5.min.js",
    "minify": "uglifyjs built/dom99.es5.js --ie8 --compress drop_console,properties,dead_code,join_vars --mangle -o built/dom99.es5.min.js",
    "buildModuleAndRequire": "node tools/buildModuleAndRequire.js",
    "build": "npm run transpile && npm run minify && npm run buildModuleAndRequire",
    "transpilePolyfills": "babel polyfills/remove.js > polyfills/built/remove.js && babel polyfills/dom-hidden.js > polyfills/built/dom-hidden.js",
    "bundleExamples": "node tools/buildExamples.js",
    "transpileExamples": "babel examples/js/built --out-dir examples/js/builtes5",
    "examples": "npm run bundleExamples && npm run transpileExamples",
    "doc": "babel documentation/js/documentation.js > documentation/js/documentation.es5.min.js",
    "yesno": "rollup components/yesNoDialog/examples/js/ScrollTest.js --o components/yesNoDialog/examples/js/built/ScrollTest-bundled.js --f iife && babel components/yesNoDialog/examples/js/built/ScrollTest-bundled.js > components/yesNoDialog/examples/js/built/ScrollTest.js",
    "buildAll": "npm run build && npm run transpilePolyfills && npm run examples && npm run doc && npm run yesno"
  },
  "babel-comment": "now transform-object-rest-spread is not used",
  "babel": {
    "plugins": [],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 0.1%",
              "last 3 versions",
              "ie 10",
              "ie 11"
            ]
          }
        }
      ]
    ]
  },
  "keywords": [
    "HTML",
    "DOM",
    "declarative",
    "custom",
    "elements",
    "web",
    "component",
    "light",
    "browser"
  ]
}
