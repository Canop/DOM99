{
  "name": "dom99",
  "version": "7.2.5",
  "url": "https://cdn.rawgit.com/GrosSacASac/DOM99/master/documentation/documentation.html#about",
  "license": "BSL-1.0",
  "licenseLink": "http://www.boost.org/LICENSE_1_0.txt",
  "description": "HTML/DOM manipulation made easier",
  "main": "built/dom99Require.js",
  "module": "built/dom99Module.js",
  "scriptFileBrowser": "js/dom99.js",
  "scriptFileBrowserES5": "built/dom99.es5.min.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/DOM99.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-polyfill": "^6.26.0",
    "uglify-js": "2.6.2",
    "utilsac": "^2.0.0",
    "rollup": "^0.50.0"
  },
  "scripts": {
    "buildes5": "babel js/dom99.js > built/dom99.es5.js",
    "buildes5minFAIL": "babel minified=true js/dom99.js > built/dom99.es5.min.js",
    "buildminify": "uglifyjs built/dom99.es5.js --screw-ie8 --compress drop_console,properties,dead_code,join_vars --mangle -o built/dom99.es5.min.js",
    "buildModuleAndRequire": "node tools/buildModuleAndRequire.js",
    "build": "npm run buildes5 && npm run buildminify && npm run buildModuleAndRequire",
    
    "transpilePolyfills": "babel polyfills/remove.js > polyfills/built/remove.js && babel polyfills/dom-hidden.js > polyfills/built/dom-hidden.js",
    
    "bundleExamples": "node tools/buildExamples.js",
    "transpileExamples": "babel examples/js/built --out-dir examples/js/builtes5",
    "buildExamples": "npm run bundleExamples && npm run transpileExamples",
    
    "builddoc": "babel documentation/js/documentation.js | uglifyjs - --screw-ie8 --compress,properties,dead_code,join_vars --mangle -o documentation/js/documentation.es5.min.js",
    
    "bundleyesnodialogUSELESS": "rollup components/yesNoDialog/yesNoDialog.js --o components/yesNoDialog/built/yesNoDialog-bundled.js --f iife",
    "buildyesnodialogexample": "rollup components/yesNoDialog/examples/js/ScrollTest.js --o components/yesNoDialog/examples/js/built/ScrollTest-bundled.js --f iife && babel components/yesNoDialog/examples/js/built/ScrollTest-bundled.js > components/yesNoDialog/examples/js/built/ScrollTest.js",
   
    "buildAll": "npm run build && npm run transpilePolyfills && npm run buildExamples && npm run builddoc && npm run buildyesnodialogexample"
  },
  "keywords": [
    "HTML",
    "DOM",
    "declarative",
    "custom",
    "elements",
    "web",
    "component",
    "fast",
    "light",
    "browser"
  ]
}
